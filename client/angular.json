{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "orion-client",
  "defaultProject": "client",
  "projects": {
    "client": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./webpack.config.js",
              "replaceDuplicatePlugins": true
            },
            "outputPath": "build",
            "baseHref": "/",
            "deployUrl": "/",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/assets/images",
              "src/assets/fonts"
            ],
            "styles": [
              "node_modules/vis-network/styles/vis-network.css",
              "src/styles.scss"
            ],
            "scripts": [
              "src/assets/libs/bootstrap.min.js"
            ]
          },
          "configurations": {
            "production": {
              "aot": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "15MB",
                  "maximumError": "15MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "12MB",
                  "maximumError": "12MB"
                }
              ],
              "outputHashing": "all",
              "optimization": {
                "styles": {
                  "inlineCritical": false
                },
                "scripts": true,
                "fonts": true
              },
              "sourceMap": {
                "scripts": true,
                "styles": true,
                "vendor": false
              },
              "extractLicenses": true,
              "namedChunks": false,
              "assets": [
                "src/favicon.ico",
                "src/assets",
                "src/assets/images",
                "src/assets/fonts"
              ],
              "styles": [
                "node_modules/vis-network/styles/vis-network.css",
                "src/styles.scss"
              ],
              "scripts": [
                "src/assets/libs/bootstrap.min.js"
              ]
            },
            "development": {
              "aot": true,
              "optimization": false,
              "sourceMap": {
                "scripts": true,
                "styles": true,
                "vendor": false
              },
              "extractLicenses": false,
              "namedChunks": true
            },
            "instrumented": {
              "customWebpackConfig": {
                "path": "./webpack.instrument.js",
                "replaceDuplicatePlugins": true
              },
              "optimization": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "proxyConfig": "proxy.conf.json",
            "hmr": false
          },
          "configurations": {
            "production": {
              "buildTarget": "client:build:production"
            },
            "development": {
              "buildTarget": "client:build:development"
            },
            "instrumented": {
              "buildTarget": "client:build:instrumented"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "client:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing",
              "@angular/localize/init"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets/images",
              "src/assets",
              "src/assets/fonts"
            ],
            "styles": [
              "node_modules/vis-network/styles/vis-network.css",
              "src/styles.scss"
            ],
            "scripts": [
              "src/assets/libs/bootstrap.min.js"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "dfd609df-b565-4930-abb7-e843ec5b6e42"
  }
}
